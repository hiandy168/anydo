/**calendar.wxss**/
.date-item{
  height: 50px;
  width: 40px;
  margin: 0 5px;
  text-align: center;
  display: inline-block;
}
.year-mouth{
  color: #2AA0C6;
  height: 2rem;
  line-height: 2rem;
  text-align: center;
}
.date-item-checked{
  color: #2AA0C6;
  border-bottom: 2px solid #2AA0C6;
}
.wek{
  height: 25px;
  line-height: 25px;
  font-size: 12px;
}
.day{
  height: 25px;
  line-height: 25px;
  font-size: 14px;
}

